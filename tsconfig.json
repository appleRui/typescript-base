{
  "compilerOptions": {
    "target": "es2016",
    "lib": ["es6","dom"],
    "module": "commonjs",
    "allowJs": true,
    "rootDir": "src",
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "outDir": "lib",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
  }
}
